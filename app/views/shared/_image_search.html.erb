<ul class="list-no-styling">
  <% @images.each do |image| %>
    <% if image.image_stored? %>
      <li class="img-library">
        <%= image_tag image.image.thumb('300x300#').url, class: "img-select img-responsive img-thumbnail", data: { source: image.image.url, id: image.id } %>
      </li>
    <% end %>
  <% end %>
</ul>
