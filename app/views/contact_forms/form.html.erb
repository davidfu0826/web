<%= bootstrap_nested_form_for @contact_form do |f| %>

  <%= f.fields_for :questions do |question_form| %>
    <%= question_form.text_field :content %>
    <%= question_form.link_to_remove t('.remove_question') %>
  <% end %>
  <%= f.link_to_add t('.add_question'), :questions %>

  <%= f.submit %>
<% end %>
