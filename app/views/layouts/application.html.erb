<!DOCTYPE html>
<html>
  <head>
    <title>TLTH</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'navbar' %>
    <div class="panorama-wrapper">
      <%= image_tag 'white_stand_se.svg', id: "logo" %>
      <%= image_tag 'http://placehold.it/1920x600', id: "panorama", class: "img-responsive center-block" %>
    </div>
    <div class="container" id="content">
      <% flash.each do |name, msg| %>
        <div class="<%= flash_class(name) %>"><%= msg %></div>
      <% end %>
      <%= yield %>
    </div>
    <%= render 'guilds' %>
    <%= render 'footer' %>
  </body>
</html>

