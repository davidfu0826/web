<div class="photo photo-small">
  <%= content_tag(:div, class: 'img img-select',
      data: {'image-source' => image.url, 'image-id' => image.id}) do %>
    <%= image_tag image.image.thumb('160x160#').url %>
    <div class="img-title">
      <%= image.title %>
    </div>
    <div class="overlay">
      <div class="links">
        <%= link_to '#' do %>
          <i class="glyphicon glyphicon-ok"></i>
        <% end %>
      </div>
    </div>
  <% end %>
</div>
